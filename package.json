{
  "name": "ai_etsy",
  "version": "1.0.0",
  "description": "AI Etsy Store",
  "scripts": {
    "prettier": "prettier --list-different './**/*.{js,ts}' --config '../.prettierrc.json' --ignore-path '../.prettierignore'",
    "prettier-write": "prettier --list-different './**/*.{js,ts}' --config '../.prettierrc.json' --ignore-path '../.prettierignore' --write",
    "typecheck": "tsc --noEmit",
    "build": "npx tsc",
    "generate-images": "npx tsc && node ./.build/handlers/generate-images/index.js",
    "generate-listing": "npx tsc && node ./.build/handlers/generate-listing/index.js"
  },
  "engines": {
    "node": ">=20.12.2"
  },
  "devDependencies": {
    "@types/node-cron": "^3.0.11",
    "@types/smb2": "^0.2.4",
    "@types/yargs": "^17.0.33",
    "prettier": "3.3.3",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.4.5",
    "http-status-codes": "^2.2.0",
    "mongodb": "^6.9.0",
    "node-cron": "^3.0.3",
    "openai": "^4.58.2",
    "sharp": "^0.33.5",
    "smb2": "^0.2.11",
    "yargs": "^17.7.2",
    "zod": "^3.23.8"
  }
}
